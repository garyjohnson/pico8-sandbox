pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

screen_size=128
fps=30

head_x=3 head_y=3
spr_size=8

spine_spr=0
head_spr=1
blink_spr=4
fire_spr=12
body_spr={0,8,24,16}

state_blink=2
state_shoot=3
state_run=4

blink_interval_sec=5
fire_speed=2

head_blink_anim={
  {blink_spr,5},
  {head_spr,5},
  {blink_spr,5},
  {head_spr,1}
}

head_shoot_anim={
  {head_spr+1,2},
  {head_spr+2,4},
  {head_spr+1,1},
  {head_spr,1},
}

head_run_anim={
  {head_spr+4,16},
}

body_blink_anim={
  {{0,8,24,24},16}
}

body_shoot_anim={
  {{24,8,24,16},2},
  {{48,8,24,16},3},
  {{24,8,24,16},2},
  {{0,8,24,16},1}
}

body_run_anim={
  {{0,8,24,16},2},
  {{0,32,24,16},2},
  {{24,32,24,16},2},
  {{0,32,24,16},2},
  {{0,8,24,16},2},
  {{0,48,24,16},2},
  {{24,48,24,16},2},
  {{0,48,24,16},1},
}

fire_anim={
  {fire_spr,2},
  {fire_spr+1,2},
  {fire_spr+2,3},
  {fire_spr+1,2},
  {fire_spr+3,2},
  {fire_spr+1,3},
}

char_state=0
frame_counter=0
fireballs={}
head_anim_start=0
head_anim={}
head_anim_spr=head_spr
body_anim_spr=body_spr

function _update()
  frame_counter+=1

  if btn(1) and char_state ~= state_run then
    start_char_anim(state_run, head_run_anim, body_run_anim)
  elseif not btn(1) and char_state == state_run then
    end_char_anim()
  end

  if btnp(5) then
    start_char_anim(state_shoot, head_shoot_anim, body_shoot_anim)
    spawn_fireball()
  end

  if char_state==0 and frame_counter >= blink_interval_sec * fps then
    start_char_anim(state_blink, head_blink_anim, body_blink_anim)
  end

  update_fireballs()
  update_char_anim()
end

function _draw()
  rectfill(0, 0, screen_size, screen_size, 0)
  spr(head_anim_spr, spr_size*head_x, spr_size*head_y)
  spr(spine_spr, spr_size*(head_x-1), spr_size*head_y)

  sspr(body_anim_spr[1],body_anim_spr[2],body_anim_spr[3],body_anim_spr[4],(spr_size*(head_x-1)), (spr_size*(head_y+1)))

  draw_fireballs()
end

function spawn_fireball()
  fireball = {}
  fireball.x = (spr_size*(head_x+1))-4
  fireball.y = (spr_size*head_y)+1
  fireball.spr = fire_spr
  fireball.anim_frame = 0
  add(fireballs, fireball)
end

function update_fireballs()
  for fireball in all(fireballs) do
    fireball.x+=fire_speed
    fireball.spr = anim_spr(fire_anim, fireball.anim_frame, true)
    fireball.anim_frame += 1

    if fireball.x > screen_size+spr_size then
      del(fireballs, fireball)
    end
  end
end

function draw_fireballs()
  for fireball in all(fireballs) do
    spr(fireball.spr, fireball.x, fireball.y)
  end
end

function anim_spr(anim, current_frame, looped)
  total_dur=0
  for v in all(anim) do total_dur+=v[2] end

  if looped then
    current_frame = current_frame % total_dur
  end

  frame_dur=0
  for i=1,#anim do
    frame_dur+=anim[i][2]
    
    if current_frame <= frame_dur then 
      return anim[i][1]
    end
  end
    
  return nil
end

function start_char_anim(anim_id, head, body)
  end_char_anim()
  char_state=anim_id
  head_anim=head
  body_anim=body
end

function update_char_anim()
  if char_state == 0 then
    return
  end

  looping = char_state == state_run
  head_anim_spr = anim_spr(head_anim, char_anim_start, looping)
  body_anim_spr = anim_spr(body_anim, char_anim_start, looping)
  if head_anim_spr == nil or body_anim_spr == nil then
    end_char_anim()
  end
  char_anim_start+=1
end

function end_char_anim()
  frame_counter=0
  char_state=0
  char_anim_start=0
  head_anim={}
  body_anim={}
  head_anim_spr=head_spr
  body_anim_spr=body_spr
end

