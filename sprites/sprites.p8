pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

screen_size=128
fps=30

head_x=3 head_y=3
spr_size=8

spine_spr=4
head_spr=5
head_state=0

frame_counter=0
blink_interval_sec=5

blink_spr=8
blink_anim_id=2
blink_anim={
  {blink_spr,5},
  {head_spr,5},
  {blink_spr,5},
  {head_spr,1}
}

shoot_anim_id=3
shoot_anim={
  {head_spr+1,2},
  {head_spr+2,4},
  {head_spr+1,1},
  {head_spr,1},
}

head_anim_start=0
head_anim={}
head_anim_spr=head_spr

fire_speed=2
fire_spr=12
fire_anim={
  {fire_spr,2},
  {fire_spr+1,2},
  {fire_spr+2,3},
  {fire_spr+1,2},
  {fire_spr+3,2},
  {fire_spr+1,3},
}

fireballs={}

function _update()
  frame_counter+=1

  if btnp(5) then
    start_head_anim(shoot_anim_id, shoot_anim)
    spawn_fireball()
  end

  if head_state==0 and frame_counter >= blink_interval_sec * fps then
    start_head_anim(blink_anim_id, blink_anim)
  end

  update_fireballs()
  update_head_anim()
end

function _draw()
  rectfill(0, 0, screen_size, screen_size, 0)
  spr(head_anim_spr, spr_size*head_x, spr_size*head_y)
  spr(spine_spr, spr_size*(head_x-1), spr_size*head_y)

  draw_fireballs()
end

function spawn_fireball()
  fireball = {}
  fireball.x = (spr_size*(head_x+1))-4
  fireball.y = (spr_size*head_y)+1
  fireball.spr = fire_spr
  fireball.anim_frame = 0
  add(fireballs, fireball)
end

function update_fireballs()
  for fireball in all(fireballs) do
    fireball.x+=fire_speed
    fireball.spr = looped_anim_spr(fire_anim, fireball.anim_frame)
    fireball.anim_frame += 1

    if fireball.x > screen_size+spr_size then
      del(fireballs, fireball)
    end
  end
end

function draw_fireballs()
  for fireball in all(fireballs) do
    spr(fireball.spr, fireball.x, fireball.y)
  end
end

function looped_anim_spr(anim, current_frame)
  total_dur=0
  for v in all(anim) do total_dur+=v[2] end

  current_frame = current_frame % total_dur

  frame_dur=0
  for i=1,#anim do
    frame_dur+=anim[i][2]
    
    if current_frame <= frame_dur then 
      return anim[i][1]
    end
  end
    
  return anim[1][1]
end

function start_head_anim(anim_id, anim)
  end_head_anim()
  head_state=anim_id
  head_anim=anim
end

function update_head_anim()
  frame_dur=0
  for i=1,#head_anim do
    frame_dur+=head_anim[i][2]
    
    if head_anim_start <= frame_dur then
      head_anim_spr = head_anim[i][1]

      if i==#head_anim and frame_dur == head_anim_start then
        end_head_anim()
      end

      break
    end

  end

  head_anim_start+=1
end

function end_head_anim()
  frame_counter=0
  head_state=0
  head_anim_start=0
  head_anim={}
  head_anim_spr=head_spr
end

